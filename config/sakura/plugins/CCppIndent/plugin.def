;プラグイン定義ファイルv1
[Plugin]
Id=net.sf.moca_skr.plugin.CCppIndent
Name=Cファミリ{}インデント
Description=C/C++/C#/JavaScript用{インデント
Type=wsh
Version=0.1
Url=
Author=moca_skr

[Plug]
SmartIndent=ccppindent.js
SmartIndent.Label=Cファミリ{}

[Wsh]
UseCache=1

[Option]
O[1].Section=Option
O[1].Key=IndentMode
O[1].Label=タブ／空白
O[1].Type=Sel
O[1].Select=タブ1つ(幅設定無視):0|空白:1|タブ+空白:2
O[2].Key=IndentWidth
O[2].Label=インデント幅
O[2].Type=Int
O[3].Key=IndentPrevData
O[3].Label=既存インデント部分も置換
O[3].Type=Bool
